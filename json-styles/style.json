{
  "QtBinding": "PySide6",
  "ShowLogs": true,
  "LiveCompileQss": true,
  "CheckForMissingicons": true,
  "QMainWindow": [
    {
      "tittle": "Nestify Home Assistant",
      "icon": ":/font_awesome_brands/icons/font_awesome/brands/airbnb.png",
      "frameless": false,
      "transluscentBg": true,
      "sizeGrip": true,
      "navigation": [
        {
          "minimize": "minimizeBtn",
          "close": "closeBtn",
          "moveWindow": "header",
          "tittleBar": "header",
          "restore": [
            {
            "buttonName": "restoreBtn",
            "normalIcon": ":/feather/Icons/feather/square.png",
            "maximizedIcon": ":/feather/Icons/feather/copy.png"
            }
          ]
        }
      ]
    }
  ],
  "QSettings": [
    {
      "ThemeSettings": [
        {
          "CustomTheme": [
            {
              "Background-color": "#FFFFFF",
              "Text-color": "#010000",
              "Icons-color": "#000000",
              "Accent-color": "#26bae3",
              "Theme-name": "LightBlue",
              "Default-Theme": true
            },
            {
              "Background-color": "#21272a",
              "Text-color": "#fefefe",
              "Icons-color": "#fefefe",
              "Accent-color": "#fba43b",
              "Theme-name": "DarkYellow",
              "Default-Theme": false
            },
            {
              "Background-color": "#1F233A",      
              "Text-color": "#FFFFFF",            
              "Icons-color": "#FFFFFF",            
              "Accent-color": "#F8757C",        
              "Theme-name": "DarkBlueAccent",     
              "Default-Theme": false               
            }

          ]
        }
      ],
      "AppSettings": {
        "OrginizationName": "Nestify Home Assistant",
        "ApplicationName": "Nestify Home Assistant Company",
        "OrginizationDormain": "Nestify Home Assistant.org"
      }
    }
  ],
  "QPushButtonGroup": [
	  {
		"Buttons": [
		  "homeBtn",
		  "voiceBtn",
		  "scenarioBtn",
		  "settingBtn"
		],
		"ActiveButton": "homeBtn",
		"Style": [
		  {
			"Active": "QPushButton{background-color: THEME.COLOR_ACCENT_1; font-weight: bold; border-top: 6px solid $COLOR_BACKGROUND_3; border-left: 6px solid $COLOR_BACKGROUND_3; border-bottom: 6px solid $COLOR_BACKGROUND_3; border-right: 6px solid $COLOR_BACKGROUND_3;}",
			"NotActive": "QPushButton{background-color: transparent; font-weight: normal; border-top: 6px solid transparent; border-left: 6px solid transparent;border-bottom: 6px solid transparent; border-right: 6px solid transparent;}"
      }
		]
	  }
	],
  "QCustomSlideMenu": [
	  {
		"name": "leftMenu",
		"defaultSize": [
		  {
			"width": 60,
			"height": "parent"
		  }
		],
		"collapsedSize": [
		  {
			"width": 60,
			"height": "parent"
		  }
		],
		"expandedSize": [
		  {
			"width": 400,
			"height": "parent"
		  }
		],
		"toggleButton": [
		  {
			"buttonName": "menuBtn",
			"icons": [
			  {
				"whenMenuIsCollapsed": ":/feather/icons/feather/align-justify.png",
				"whenMenuIsExpanded": ":/feather/icons/feather/arrow-left-circle.png"
			  }
			]
		  }
		],
		"menuTransitionAnimation": [
		  {
			"animationDuration": 500,
			"animationEasingCurve": "Linear"
		  }
		]
	  }
  ],
  "QCustomQStackedWidget": [
	  {
		"name": "mainPages",
		"navigation": [
		  {
			"navigationButtons": [
			  {
				"homeBtn": "homePage",
				"scenarioBtn": "scenarioPage",
				"voiceBtn": "voiceassisPage",
				"settingBtn": "settingPage"
			  }
			]
		  }
		],
		"transitionAnimation": [
		  {
			"fade": [
			  {
				"active": true,
				"duration": 500,
				"easingCurve": "InSine"
			  }
			]
		  }
		]
	  }
	]
}
